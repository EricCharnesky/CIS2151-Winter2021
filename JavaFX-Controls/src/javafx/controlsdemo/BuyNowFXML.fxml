<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.controlsdemo.BuyNowFXMLController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <top>
            <Label fx:id="textLabel" text="Let's play a game!" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Comic Sans MS" size="36.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="textLabel" text="Label" />
               </children>
            </HBox>
         </bottom>
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#rollButtonClicked" text="Roll" />
               </children>
            </VBox>
         </left>
         <right>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
         </right>
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <CheckBox fx:id="firstCheckBox" mnemonicParsing="false" text="hold" GridPane.rowIndex="1" />
                  <CheckBox fx:id="secondCheckBox" mnemonicParsing="false" text="hold" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <CheckBox fx:id="thirdCheckBox" mnemonicParsing="false" text="hold" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <CheckBox fx:id="fourthCheckBox" mnemonicParsing="false" text="hold" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <CheckBox fx:id="fifthCheckBox" mnemonicParsing="false" text="hold" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                  <Label fx:id="firstLabel" />
                  <Label fx:id="secondLabel" GridPane.columnIndex="1" />
                  <Label fx:id="thirdLabel" GridPane.columnIndex="2" />
                  <Label fx:id="fourthLabel" GridPane.columnIndex="3" />
                  <Label fx:id="fifthLabel" GridPane.columnIndex="4" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
