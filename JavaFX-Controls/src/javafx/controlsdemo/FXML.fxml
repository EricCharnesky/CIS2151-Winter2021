<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.controlsdemo.FXMLController">
   <children>
      <Slider fx:id="tensSlider" blockIncrement="1.0" layoutX="85.0" layoutY="200.0" majorTickUnit="1.0" max="9.0" minorTickCount="0" prefHeight="24.0" prefWidth="409.0" showTickMarks="true" snapToTicks="true" />
      <Label fx:id="textLabel" layoutX="220.0" layoutY="102.0" text="\$0">
         <font>
            <Font name="Comic Sans MS" size="36.0" />
         </font>
      </Label>
      <Label layoutX="28.0" layoutY="204.0" text="\$10s" />
      <Slider fx:id="onesSlider" blockIncrement="1.0" layoutX="85.0" layoutY="232.0" majorTickUnit="1.0" max="9.0" minorTickCount="0" prefHeight="24.0" prefWidth="409.0" showTickMarks="true" snapToTicks="true" />
      <Label layoutX="28.0" layoutY="236.0" text="\$1s" />
      <TextField fx:id="textField" layoutX="159.0" layoutY="286.0" onKeyTyped="#textChanged" text="0" />
      <Label layoutX="147.0" layoutY="287.0" text="\$">
         <font>
            <Font name="Comic Sans MS" size="17.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="159.0" layoutY="319.0" textFill="RED" />
      <Button fx:id="buyNowButton" layoutX="218.0" layoutY="346.0" mnemonicParsing="false" onAction="#buyNowButtonClicked" text="Buy Now!" />
   </children>
</AnchorPane>
